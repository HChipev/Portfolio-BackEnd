{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "SqlConnection": "{From environment variable}}"
  },
  "Jwt": {
    "Issuer": "HChipev",
    "Audience": "Public",
    "Key": "{From environment variable}",
    "RefreshTokenValidityInDays": 30,
    "TokenValidityInMinutes": 450
  },
  "Communication": {
    "Email": {
      "TemplatePath": "../Common/Templates/Email.html",
      "ServiceEmail": "{From environment variable}",
      "SendGridApiKey": "{From environment variable}",
      "RecipientEmail": "{From environment variable}"
    }
  }
}
